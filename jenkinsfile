pipeline {
    agent any
    stages {
        stage ('Build dockerfile') {
            steps{
                sh "docker build -t pipeline2 ."
            }
        } 
        stage ('push dockerfile') {
            steps {
                sh "echo $dockerhub | docker login -u yigalisaac --password-stdin"
                sh "docker push pipeline2:latest ."

            }

        }

    }
}